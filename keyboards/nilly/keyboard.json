{
    "manufacturer": "nohwnd",
    "keyboard_name": "nilly",
    "maintainer": "nohwnd",
    "bootloader": "rp2040",
    "diode_direction": "ROW2COL",
    "features": {
        "bootmagic": true,
        "command": false,
        "console": true,
        "extrakey": true,
        "mousekey": true,
        "nkro": true
    }, 
    // Left side
    // col 0 - orange = green white
    // col 1 - orange white = broqn white 
    // col 2 - green -brown
    //
    // row 0 - blue = blue white
    // row 1 - brown = green
    // row 2 - brown white = orange white
    // row 3 - green white = orange
    // row 4 - blue white = blue
    // 
    // Right side is wired the same, and mapped to matrix starting with index 6.
    "matrix_pins": {
        "cols": [
            // orange, orange white, green, 
            // "GP0", "GP0", "GP1", "GP1", "GP2", "GP2",     "GP14", "GP14", "GP15", "GP15", "GP26", "GP26"
            "GP6", "GP6", "GP5", "GP5", "GP4", "GP4",   "GP14", "GP14", "GP15", "GP15", "GP26", "GP26"
        ],
        "rows": [

            // Left side
            // "GP3", // blue 
            "GP7",
            // "GP4", // brown
             "GP2",
            // "GP5", // brown white
            "GP1",

            "GP9", // unused (not connected) - rotary encoder reserved row, this decision was made by cheapino which this keyboard is based on

            //  "GP6", // green white
             "GP0",
            //"GP7", // blue white
             "GP3",

            // right side
            "GP11", //  "GP27",
            "GP28",
            "GP29",
            "GP8",
            "GP9"
        ]
    },
    "processor": "RP2040",
    "usb": {
        "device_version": "0.0.1",
        "pid": "0x0000",
        "vid": "0xFEED"
    },
    "layouts": {
        "LAYOUT": {
            "layout": [
                { "matrix": [0, 0], "y": 0, "x": 0 },
                { "matrix": [0, 1], "y": 0, "x": 1 },
